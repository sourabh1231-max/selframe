package TEST;

public class Oddnumber {

	public static void main(String[] args) { 
		// TODO Auto-generated method stub
        int i=111122;
        
		if(i <1)
			
		{
			System.out.print("not a odd numeber");
		}
		
		else if(i%2==0)
		{
			System.out.print("not a odd number");
		}
		else if(i%2>0)
		{
			System.out.print(" odd number");
			
		}
	}

}
