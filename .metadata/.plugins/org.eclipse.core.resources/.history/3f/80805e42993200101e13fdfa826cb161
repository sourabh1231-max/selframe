package TEST;

public class fib3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int first=0; 
		int second=0;
	    int n=20;
	    
	    for(int j=0;j<=n;j++)
	    {
	    	System.out.print(j);
	    	int nxt=first+second;
	    	j=second;
	    	second=nxt;
	    	
	    
	    }
	}

}
